<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="scheduler">
        <job name="scheduler_cron_processor" instance="MageSuite\Schedule\Cron\Process" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
